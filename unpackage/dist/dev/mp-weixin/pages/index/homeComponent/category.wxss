.category_home {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
      -ms-flex-direction: row;
          flex-direction: row;
  -webkit-flex-wrap: wrap;
      -ms-flex-wrap: wrap;
          flex-wrap: wrap;
}
.category_home .category_list {
    width: 345rpx;
    height: 100rpx;
    position: relative;
    border-radius: 10rpx;
    -webkit-box-flex: 0;
    -webkit-flex: 0 0 345rpx;
        -ms-flex: 0 0 345rpx;
            flex: 0 0 345rpx;
    margin-bottom: 20rpx;
}
.category_home .category_list:nth-child(even) {
      margin-left: 20rpx;
}
.category_home .category_list .category_text {
      text-align: center;
      line-height: 100rpx;
      font-size: 30rpx;
      text-align: center;
      display: block;
      color: #fff;
}
.category_home .category_list .category_backgorund {
      position: absolute;
      background: url(data:image/png;base64,) no-repeat;
      width: 100%;
      height: 100%;
      background-size: cover;
      top: 0;
      left: 0;
}
.category_home .upside_list_class {
    width: 100%;
    -webkit-box-flex: 0;
    -webkit-flex: 0 0 100%;
        -ms-flex: 0 0 100%;
            flex: 0 0 100%;
}
.category_home .upside_list_class .img {
      background: url(data:image/png;base64,) no-repeat;
}

