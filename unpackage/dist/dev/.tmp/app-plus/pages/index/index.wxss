.service,
.notice_annount,
.category_home,
.news_prople,
.hot_activity {
	padding: 0 20rpx;
}
.page{
	background:#ffffff;
}
.test {
	font-family: iconfont;
	margin-left: 20rpx;
}
.banner {
	width: 100%;
	height: 320rpx;
}
/**
 * 客服 
 */
.service {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: reverse;
	-webkit-flex-direction: row-reverse;
	    -ms-flex-direction: row-reverse;
	        flex-direction: row-reverse;
	height: 70rpx;
}
.service-input {
	-webkit-align-self: center;
	    -ms-flex-item-align: center;
	        align-self: center;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.service-icon {
	font-size: 42rpx;
	width: 50rpx;
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 50rpx;
	    -ms-flex: 0 0 50rpx;
	        flex: 0 0 50rpx;
	color: #2580EF;
}
.service-text {
	font-size: 30rpx;
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 120rpx;
	    -ms-flex: 0 0 120rpx;
	        flex: 0 0 120rpx;
	width: 120rpx;
	color: #2580EF;
	line-height: 45rpx;
}
/**
 * 分类
 */
.category_home {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	    -ms-flex-direction: row;
	        flex-direction: row;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.category_list {
	width: 345rpx;
	height: 100rpx;
	position: relative;
	border-radius: 10rpx;
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 345rpx;
	    -ms-flex: 0 0 345rpx;
	        flex: 0 0 345rpx;
	margin-bottom: 20rpx
}
.category_list:nth-child(even) {
	margin-left: 20rpx;
}
.category_text {
	text-align: center;
	line-height: 100rpx;
	font-size: 30rpx;
	text-align: center;
	display: block;
	color: #fff;
}
.category_backgorund {
	position: absolute;
	background: url(data:image/png;base64,) no-repeat;
	width: 100%;
	height: 100%;
	background-size: cover;
	top: 0;
	left: 0;
}
.upside_list_class {
	width: 100%;
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 100%;
	    -ms-flex: 0 0 100%;
	        flex: 0 0 100%;
}
.upside_list_class .img {
	background: url(data:image/png;base64,) no-repeat;
}
/*
** 新手引导
*/
.news_prople .news_title{
	font-size:30rpx;
}
.news_prople .news_images{
	width:100%;
	height:52rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size:cover;
	-webkit-box-shadow:0 -4rpx 3rpx 2rpx rgba(0,0,0,.05), 0 5rpx 5rpx 0 #afbae5;
	        box-shadow:0 -4rpx 3rpx 2rpx rgba(0,0,0,.05), 0 5rpx 5rpx 0 #afbae5;
	margin-top:15rpx;
}
.news_prople .news_text{
	font-size:26rpx;
	line-height:52rpx;
	text-align:center;
	display:block;
}
/*
** 热门活动
*/
.hot_activity{
	margin-top:20rpx;
}
.hot_activity .activity_title{
	width:100%;
	display:-webkit-box;
	display:-webkit-flex;
	display:-ms-flexbox;
	display:flex;
	-webkit-box-orient:horizontal;
	-webkit-box-direction:normal;
	-webkit-flex-direction:row;
	    -ms-flex-direction:row;
	        flex-direction:row;
}
.hot_activity .activity_text{
	font-size:30rpx;
	padding-top:8rpx;
}
.hot_activity .activity_image{
	width:320rpx;
	height:50rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size:100% 100%;
	font-size:26rpx;
	text-align:center;
	color:#fff;
	line-height:50rpx;
}
.activity_box .activity_list{
	width:100%;
	height:176rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size:100% 100%;
	margin:20rpx 0 10rpx 0;
	display:-webkit-box;
	display:-webkit-flex;
	display:-ms-flexbox;
	display:flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	    -ms-flex-direction: row;
	        flex-direction: row;
}
.activity_box .activity{
	-webkit-box-flex:1;
	-webkit-flex:1;
	    -ms-flex:1;
	        flex:1;
}
.activity_box .money_num{
	font-size:92rpx;
	line-height:176rpx;
	text-align:center;
	display:block;
}
.activity_box .day_money{
	font-size:28rpx;
	width:160rpx;
	text-align:center;	
	margin-top:50rpx;
}
.activity_box .box_money_unit{
	font-size:36rpx;
	color:#fff;
}
.activity_box .task{
	font-size:28rpx;
	margin:20rpx 0 10rpx 0;
	text-align:center;
}
.task text{
	display:block;
}
.activity_box .to_detail{
	width:140rpx;
	height:50rpx;
	color: #dda643;
	background-color: #fdfbf6;
	border-radius:100rpx;
	background: -webkit-gradient(linear,left top, left bottom,from(#fdfbf6),to(#fbe3ad));
	background: -o-linear-gradient(top,#fdfbf6,#fbe3ad);
	background: linear-gradient(-180deg,#fdfbf6,#fbe3ad);
	font-size:28rpx;
	line-height:50rpx;
	text-align:center;
	margin:0 auto;
	cursor:pointer;
}

