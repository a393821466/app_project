<view class="{{['login',className]}}"><view class="logo"><image class="logo_img" src="../../static/images/yh1.png"></image><text class="logo_txt">欢迎来到期货交易通</text></view><view class="login_form"><view class="login_form_node login_form_userPhone"><view class="loginsIcon"><fonts-icon vue-id="1" type="huiyuanzhongxin" color="#666" bind:__l="__l"></fonts-icon></view><input class="loginInput userPhone" type="number" maxlength="11" placeholder="请输入手机号码" value="{{ruleForm.username}}"/></view><view class="login_form_node login_form_userPassword"><view class="loginsIcon"><fonts-icon vue-id="2" type="suo" color="#666" bind:__l="__l"></fonts-icon></view><input class="loginInput userPassword" type="password" value="" placeholder="请输入密码"/><view class="login_hide_pwd"><fonts-icon vue-id="3" type="yanjing1" color="#666" data-event-opts="{{[['tap',[['onClickEye',['$event']]]]]}}" bindtap="__e" bind:__l="__l"></fonts-icon></view></view><view class="login_form_forgetPassword"><label class="boxCheckBoxView"><view class="checkBoxStyle"><checkbox style="transform:scale(0.8);" value="" checked></checkbox></view><view class="checkBoxText">记住密码?</view></label><view class="forget_password">忘记密码?</view></view><view class="login_submit"><button class="login_submit_int" type="primary" loading="{{btnLoading}}" data-event-opts="{{[['tap',[['login_btn',['$event']]]]]}}" bindtap="__e">登陆</button></view></view></view>